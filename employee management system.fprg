                                                                            <input variable="emploeventmonth"/>
                                                                            <output expression="&quot;event has been updated&quot;" newline="True"/>
                                                                            <assign variable="w" expression="1"/>
                                                                        </then>
                                                                        <else>
                                                                            <if expression="m==6">
