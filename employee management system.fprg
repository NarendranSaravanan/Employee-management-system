                                                                                                        <input variable="emploperf[serialnum]"/>
                                                                                                        <output expression="&quot;press 1 to enter other employees performance report or press anyother key to exit&quot;" newline="True"/>
                                                                                                        <input variable="performance"/>
                                                                                                        <assign variable="perf" expression="1"/>
                                                                                                    </do>
                                                                                                </then>
                                                                                                <else>
                                                                                                    <if expression="m==9">
                                                                                                        <then>
                                                                                                            <output expression="&quot;logout&quot;" newline="True"/>
                                                                                                        </then>
                                                                                                        <else/>
                                                                                                    </if>
                                                                                                </else>
                                                                                            </if>
                                                                                        </else>
                                                                                    </if>
                                                                                </else>
                                                                            </if>
                                                                        </else>
                                                                    </if>
                                                                </else>
                                                            </if>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                    <output expression="&quot;press 1 to adminstrative pannel&quot;" newline="True"/>
                                    <output expression="&quot; any other to exit&quot;" newline="True"/>
                                    <input variable="z"/>
                                </do>
                            </then>
                            <else>
                                <breakpoint/>
                            </else>
                        </if>
                    </else>
                </if>
                <output expression="&quot;press 1 to enter main menu or anyother key to exit&quot;" newline="True"/>
                <input variable="y"/>
            </do>
        </body>
    </function>
    <function name="editinfo" type="None" variable="">
        <parameters>
            <parameter name="serialnum" type="Integer" array="False"/>
            <parameter name="infoadd" type="String" array="True"/>
            <parameter name="salarygiven" type="Integer" array="True"/>
            <parameter name="payments" type="Integer" array="True"/>
            <parameter name="dedection" type="Integer" array="True"/>
        </parameters>
        <body>
            <declare name="l1" type="Integer" array="False" size=""/>
            <output expression="&quot;press1 to delete general inforation&#13;&#10;press 2 to delete salary given&#13;&#10;press3 to delete payments&#13;&#10;press4 to delete dedection amount&quot;" newline="True"/>
            <input variable="l1"/>
            <if expression="l1==1">
                <then>
                    <assign variable="infoadd[serialnum]" expression="&quot;not mentioned&quot;"/>
                </then>
                <else>
                    <if expression="l1==2">
                        <then>
                            <assign variable="salarygiven[serialnum]" expression="0"/>
                        </then>
                        <else>
                            <if expression="l1==3">
                                <then>
                                    <assign variable="payments[serialnum]" expression="0"/>
                                </then>
                                <else>
                                    <if expression="l1=4">
                                        <then>
                                            <assign variable="dedection[serialnum]" expression="0"/>
                                        </then>
                                        <else/>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
