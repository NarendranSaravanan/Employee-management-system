                                                                                                                                            <output expression="&quot;reason for empolyee leave is &quot; &amp; j[s]" newline="True"/>
                                                                                                                                            <output expression="&quot;if leave is granted press 1 if denied press 2&quot;" newline="True"/>
                                                                                                                                            <input variable="leavegranted"/>
