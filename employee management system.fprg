                                                                                                                                                <then>
                                                                                                                                                    <assign variable="leaveagreed" expression="leaveagreed+1"/>
                                                                                                                                                </then>
                                                                                                                                                <else/>
                                                                                                                                            </if>
                                                                                                                                        </else>
                                                                                                                                    </if>
                                                                                                                                </then>
                                                                                                                                <else>
                                                                                                                                    <output expression="&quot;there is no request for leave&quot;" newline="True"/>
                                                                                                                                </else>
                                                                                                                            </if>
                                                                                                                        </for>
                                                                                                                    </then>
                                                                                                                    <else/>
                                                                                                                </if>
                                                                                                            </else>
                                                                                                        </if>
                                                                                                    </else>
                                                                                                </if>
                                                                                            </else>
                                                                                        </if>
                                                                                        <output expression="&quot;press 1 to manage employee leave and any puther to exit&quot;" newline="True"/>
                                                                                        <input variable="v"/>
                                                                                    </do>
                                                                                </then>
                                                                                <else>
                                                                                    <if expression="m==7">
                                                                                        <then>
                                                                                            <output expression="&quot;employee training record&quot;" newline="True"/>
