                                                                                    <do expression="v==1">
                                                                                        <output expression="&quot;to view leave granted list press 1&quot;" newline="True"/>
                                                                                        <output expression="&quot;to view dates of leave press 2&quot;" newline="True"/>
                                                                                        <output expression="&quot;to view total number of leave press 3&quot;" newline="True"/>
