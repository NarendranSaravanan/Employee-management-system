                                                                                                    <else>
                                                                                                        <if expression="o==3">
                                                                                                            <then>
                                                                                                                <output expression="&quot;total number of leave is&quot; &amp; g" newline="True"/>
                                                                                                            </then>
                                                                                                            <else>
                                                                                                                <if expression="o==4">
                                                                                                                    <then>
                                                                                                                        <for variable="s" start="0 " end="p-1" direction="inc" step="1">
                                                                                                                            <if expression="x!=0">
                                                                                                                                <then>
                                                                                                                                    <if expression="q[s]==0">
